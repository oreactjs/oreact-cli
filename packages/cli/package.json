{
  "name": "oreact-cli",
  "version": "0.1.9",
  "description": "Oreact CLI toolkit for amazing developer experience",
  "main": "lib/index.js",
  "bin": {
    "oreact": "lib/index.js"
  },
  "scripts": {
    "pretest": "rimraf ./tests/output",
    "test": "jest --forceExit"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/tests/output",
      "<rootDir>/tests/subjects"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/tests/output",
      "<rootDir>/tests/subjects"
    ]
  },
  "files": [
    "lib",
    "babel"
  ],
  "keywords": [
    "oreact",
    "init",
    "setup",
    "deploy",
    "orup",
    "cli",
    "pwa",
    "project",
    "generator"
  ],
  "author": "Vinay Kevadiya <vinaykevadiaa@gmail.com>",
  "homepage": "https://oreactjs.com/docs/cli/introduction",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^6.1.0",
    "html-looks-like": "^1.0.2",
    "jest": "^25.1.0",
    "lerna": "^3.16.4",
    "lint-staged": "^10.0.7",
    "mkdirp": "^1.0.3",
    "native-url": "0.3.1",
    "ncp": "^2.0.0",
