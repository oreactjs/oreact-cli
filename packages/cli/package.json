    "cross-env": "5.2.0",
    "cross-spawn-promise": "^0.10.1",
    "envinfo": "^7.3.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "fs": "^0.0.1-security",
    "gittar": "^0.1.0",
    "glob": "^7.1.4",
    "isomorphic-unfetch": "^3.0.0",
    "kleur": "^3.0.3",
    "lerna": "^3.16.4",
    "lint-staged": "^10.0.7",
    "ora": "^4.0.3",
